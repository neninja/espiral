digraph D {
    dev[label="cria/modifica/deleta arquivos"];

    node [shape=rect,fontname=mono];
    init[label="git init"];
    clone[label="git clone <url>"];
    add[label="git add"];
    commit[label="git commit -m \"<Mensagem de commit>\""];
    push[label="git push origin HEAD"];

    init -> dev;
    clone -> dev;

    dev -> add -> commit;

    commit -> push;
    commit -> dev;
}

